<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.geometry4me">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_product"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_product_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConeActivity" />
        <activity android:name=".CylinderActivity" />
        <activity android:name=".PyramidActivity" />
        <activity android:name=".PrismActivity" />
        <activity android:name=".SphereActivity" />
        <activity android:name=".TriangleActivity" />
        <activity android:name=".RectangleActivity" />
        <activity android:name=".CircleActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>